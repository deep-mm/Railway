<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.ImageActivity">

    <Button
        android:id="@+id/gImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="49dp"
        android:layout_marginTop="57dp"
        android:onClick="getImage"
        android:text="Get" />

    <Button
        android:id="@+id/uploadImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/gImage"
        android:layout_toStartOf="@+id/clickImage"
        android:onClick="uploadImage"
        android:text="Upload" />

    <Button
        android:id="@+id/clickImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="Click" />

    <Button
        android:id="@+id/uploadBogey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/uploadImage"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="47dp"
        android:onClick="uploadBogey"
        android:text="Bogey" />

    <Button
        android:id="@+id/uploadGeneral"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/uploadBogey"
        android:layout_alignStart="@+id/gImage"
        android:onClick="uploadGeneral"
        android:text="General" />
</RelativeLayout>