<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.amey.loginfirebase">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.StructureActivity" />
        <activity android:name=".Activity.InputActivity"></activity>
        <activity android:name=".Activity.AuthActivity"/>
        <activity android:name=".Activity.DetailedReportActivity"/>
        <activity android:name=".Activity.GeneralReportActivity"/>
        <activity android:name=".Activity.TrainActivity"/>
    </application>

</manifest>